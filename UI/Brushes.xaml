    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Border" Color="#FF312929" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Foreground" Color="White" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Background" Color="#FF251919" po:Freeze="True"/>
