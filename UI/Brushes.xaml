    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Glow.NonActive" Color="#FF121212" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Title.NonActive" Color="#FF4C3C3C" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Border.NonActive" Color="#FF333030" po:Freeze="True"/>

