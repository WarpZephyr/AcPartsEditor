    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    mc:Ignorable="po">

    <!--  Window  -->
    <!-- Normal -->
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Glow" Color="Black" po:Freeze="True"/>
