<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    mc:Ignorable="po">

    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.Border" Color="#FF312929" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.Foreground" Color="White" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.Background" Color="#FF2D1E1E" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.BorderPressed" Color="#FF312929" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.ForegroundPressed" Color="White" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.BackgroundPressed" Color="#FF3C2727" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.BorderHighlighted" Color="#FF3D2929" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.ForegroundHighlighted" Color="White" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.TopMenuItem.BackgroundHighlighted" Color="#FF3C2727" po:Freeze="True"/>

</ResourceDictionary>