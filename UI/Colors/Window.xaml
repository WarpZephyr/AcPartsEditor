<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    mc:Ignorable="po">

    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Glow" Color="Black" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Title" Color="#FF4C2727" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Border" Color="#FF312929" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Foreground" Color="White" po:Freeze="True"/>
    <SolidColorBrush x:Key="Zephyr.Brushes.Window.Background" Color="#FF251919" po:Freeze="True"/>

</ResourceDictionary>